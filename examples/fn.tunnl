fn sayHi(first, last) start
    print "Hi, " + first + " " + last + "!";
end

fn printCurrentTime() start
    print "Current Unix Time is...";
    print clock();
end

fn main() start
    tunnl name = "tunnl";
    tunnl surname = "user";

    sayHi(name, surname);
    printCurrentTime();
end

// later it'll be called automatically
main();